<p id="notice"><%= notice %></p>

<h1>Listing Donates</h1>

<table>
  <thead>
    <tr>
      <th>Amount</th>
      <th>Name</th>
      <th>Date Time</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @donates.each do |donate| %>
      <tr>
        <td><%= donate.amount %></td>>
        <td><%= donate.full_name%></td>
        <td><%=l donate.created_at%></td>
      </tr>
    <% end if @donates%>
  </tbody>
</table>

<br>

<%= link_to 'New donate', new_donate_path %>
