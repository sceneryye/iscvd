 <table class="table table-striped table-advance table-hover">
  <tbody>
    <tr>
       <th><i class="icon_profile"></i>Title</th>
       <th><i class="icon_pin_alt"></i>Location</th>
       <th><i class="icon_calendar"></i>Duration</th>
       <th><i class="icon_clock_alt"></i>Created_At</th>
       <th><i class="icon_clock_alt"></i>Updated_At</th>
    </tr>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.title_en %><br><%= meeting.title_zh %></td>
        <td><%= meeting.location_en %><br><%= meeting.location_zh %></td>
        <td>from:<%= meeting.start_time %><br>to:<%= meeting.end_time %></td>
        <td><%=meeting.created_at%></td>
        <td><%=meeting.updated_at%></td>
        <td>
          <div class="btn-group">
            <a class="btn btn-primary" href="#"><i class="icon_plus_alt2"></i></a>
            <a class="btn btn-success" href="#"><i class="icon_check_alt2"></i></a>
            <a class="btn btn-danger" href="#"><i class="icon_close_alt2"></i></a>
            <%#= link_to '<i class="icon_plus_alt2"></i>'.html_safe, admin_meeting_path , class:'btn btn-primary'%>
        <%= link_to '<i class="icon_check_alt2"></i>'.html_safe, edit_admin_meeting_path(meeting) , class:'btn btn-success' %>
        <%#= link_to '<i class="icon_close_alt2"></i>'.html_safe,  admin_meeting_url, method: :delete, data: { confirm: 'Are you sure?' }  , class:'btn btn-danger'%>
          </div>

        </td>
      </tr>
    <% end if  @meetings%>

  </tbody>
</table>
<br>

<%= link_to 'New meeting', new_admin_meeting_path %>
