<p id="notice"><%= notice %></p>

<h1>Listing meeting_attendees</h1>

<%=form_for :member,:url=>export_admin_meeting_attendees_path,:method=>:post do |f|%>
        &nbsp;&nbsp;&nbsp;&nbsp;
     
        <%= f.submit '导出报表', :class => "button batch"%>
     
                    <%end%>

 <table class="table table-striped table-advance table-hover">
  <thead>
    <tr>
      <th>Email</th>
      <th>Full Name</th>
      <th>name</th>
<th>mobile</th>
<th>address</th>
<th>Meeting Name</th>

      <!-- <th colspan="3"></th>-->
    </tr>
  </thead>

  <tbody>
    <% @meeting_attendees.each do |meeting_attendee| %>
      <tr>
        <th><%= meeting_attendee.email %></th>
        <th><%= meeting_attendee.user.full_name if meeting_attendee.user_id %></th>
        <th><%= meeting_attendee.name %></th>
        <th><%= meeting_attendee.user.mobile if meeting_attendee.user_id %></th>
        <th><%= meeting_attendee.user.address if meeting_attendee.user_id %></th>
        
        <th><%= meeting_attendee.meetings.title_zh if meeting_attendee.meetings_id %></th>
        <!--<th><%= link_to 'Show', meeting_attendee %></th>-->
      </tr>
    <% end if @meeting_attendess%>
  </tbody>
</table>

<br>
